{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "torquescript",
      "camelcase": "TorqueScript",
      "title": "Torque Script",
      "scope": "source.torquescript",
      "file-types": [
        "cs",
        "gui"
      ],
      "injection-regex": "^torquescript$",
      "class-name": "TreeSitterTorquescript",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "The language used in the Torque 3D Game Engine",
    "authors": [
      {
        "name": "Monoblaster"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-torquescript"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
